{
	"manifest_version": 2,
	"name": "Inetmafia.ru",
	"description": "Монитор лобби Inetmafia.ru",
	"version": "2.0.4",
	"browser_action": {
		"default_icon": {
			"19": "favicon.png",
			"38": "favicon.png"
		},
		"default_title": "Inetmafia",
		"default_popup": "popup.html"
	},
	"background": {
		"scripts": ["fetch.js"]
	},
	"permissions": [
		"http://inetmafia.ru/*",
		"https://inetmafia.ru/*"
	],
	"icons": {
		"16": "favicon.png",
		"48": "favicon.png",
		"128": "favicon.png"
	}
}